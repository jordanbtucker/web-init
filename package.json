{
  "name": "web-init",
  "version": "0.0.0",
  "description": "Starting point for web projects",
  "private": true,
  "main": "lib/server/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "engines": {
    "node": ">=8.12.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "coverage": "run-s -c spec tap-coverage",
    "dev": "concurrently -c green,blue npm:dev-server npm:dev-client",
    "dev-client": "nodemon -w .env -w package.json -w webpack.config.js -x webpack-dev-server",
    "dev-server": "nodemon -w .env -w package.json -w lib/server lib/server",
    "eslint": "eslint --fix .",
    "lint": "run-s eslint prettier",
    "prettier": "prettier --loglevel warn --write **/*.{babelrc,css,html,js,json,md,scss}",
    "preversion": "npm test",
    "spec": "run-s build tap",
    "start": "cross-env NODE_ENV=production node lib/server",
    "tap": "cross-env NODE_ENV=production tap test -j 1 -R spec",
    "tap-coverage": "tap --coverage-report html",
    "test": "run-s lint spec"
  },
  "dependencies": {
    "@feathersjs/express": "^4.3.3",
    "@feathersjs/feathers": "^4.3.3",
    "@feathersjs/socketio": "^4.3.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express-async-handler": "^1.1.4",
    "feathers-memory": "^4.0.1",
    "helmet": "^3.21.1",
    "morgan": "^1.9.1",
    "serve-static": "^1.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@feathersjs/socketio-client": "^4.3.3",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.3.1",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.2",
    "core-js": "^3.2.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.15.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.7.0",
    "jquery": "^3.4.1",
    "lint-staged": "^9.4.2",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.3",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.15.0",
    "prettier": "^1.18.2",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "sass-loader": "^8.0.0",
    "socket.io-client": "^2.3.0",
    "style-loader": "^1.0.0",
    "supertest": "^4.0.2",
    "tap": "^14.7.1",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  }
}
